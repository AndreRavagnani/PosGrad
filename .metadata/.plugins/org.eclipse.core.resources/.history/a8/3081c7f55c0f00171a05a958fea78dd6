
public class TestePincel {
	public static void main(String[] args) {
		Pincel p1 = new Pincel();
		Pincel p2 = new Pincel();
		Pincel p3 = new Pincel();
		Pincel p4 = new Pincel();
		
		p1.cor = "Rosa";
		p1.forma = "Cilindrico";
		p1.tamanho = 10;
		p1.capacidade = 50;
		
		p2.cor = "Preto";
		p2.forma = "Cilindrico";
		p2.tamanho = 15;
		p2.capacidade = 80;
		System.out.println(p1.cor);
		System.out.println(p2.cor);
		System.out.println(p4.cor);
		p2.Riscar("Não sei");
	}
}
